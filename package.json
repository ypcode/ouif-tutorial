{
  "name": "ui-fabric-tutorial",
  "version": "1.0.0",
  "description": "A library to add scripted tutorial based on Office UI Fabric to UI components",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=webpack.config.release.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "tslint './src/**/*.ts*' --format stylish --force",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=webpack.dev.config.js"
  },
  "author": "Yannick Plenevaux",
  "license": "ISC",
  "dependencies": {
    "office-ui-fabric-react": "^5.135.0",
    "react": "16.7.0",
    "react-dom": "16.7.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "typescript": "^3.4.1",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "node-sass": "^4.11.0",
    "react-hot-loader": "^4.8.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.14.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.2.1",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  }
}
